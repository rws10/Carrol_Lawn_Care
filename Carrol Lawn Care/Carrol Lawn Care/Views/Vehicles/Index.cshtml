@model IEnumerable<Carrol_Lawn_Care.Models.Vehicle>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.licPlate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TblEquip.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.licPlate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TblEquip.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.vehId }) |
            @Html.ActionLink("Details", "Details", new { id=item.vehId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.vehId })
        </td>
    </tr>
}

</table>
